# .env.example
# Copy to .env and adjust values for your environment

# Next.js
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
TAILWIND_DISABLE_OXIDE=1

# App URLs (no trailing slash)
NEXT_PUBLIC_SITE_URL=https://pojpro.uz
SITE_URL=https://pojpro.uz
NEXTAUTH_URL=http://localhost:3000

# Auth
NEXTAUTH_SECRET=changeme

# Database
DATABASE_URL=postgres://user:pass@host:5432/dbname

# Analytics (optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
NEXT_PUBLIC_GTM_ID=
NEXT_PUBLIC_YM_ID=

# Error monitoring (optional)
SENTRY_DSN=

# Windows build workaround (optional)
# DIST_IN_NODE_MODULES=1
