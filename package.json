{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17 <23"
  },
  "scripts": {
    "clean": "node scripts/force-clean-next.js",
    "dev": "next dev",
    "build": "node scripts/sanitize-public.mjs && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "lh:desktop": "lighthouse http://localhost:3000 --only-categories=performance,seo --preset=desktop --output=json,html --output-path=./seo-reports/lh-desktop.report.html",
    "lh:mobile": "lighthouse http://localhost:3000 --only-categories=performance,seo --output=json,html --output-path=./seo-reports/lh-mobile.report.html",
    "start": "next start -p $PORT",
    "start:ci": "cross-env PORT=3000 next start -p $PORT -H 0.0.0.0",
    "start:devprod": "cross-env DISABLE_REDIS=1 next start -p 3000",
    "start:win": "next start -p 3000",
    "lint": "next lint",
    "postinstall": "prisma generate && next telemetry disable || true",
    "seed": "ts-node prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "images:fill-missing": "node scripts/fill-missing-images-from-ru.js",
    "i18n:check": "node scripts/i18n-check.mjs",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --watch --config=jest.config.cjs",
    "test:coverage": "jest --coverage --config=jest.config.cjs",
    "test:unit:seo": "jest --config=jest.config.cjs --coverage --collectCoverageFrom='src/components/seo/**/*.{ts,tsx}'",
    "test:e2e:seo": "playwright test --config=playwright.config.ts --grep @seo",
    "test:a11y": "playwright test --config=playwright.config.ts --grep @a11y",
    "test:i18n": "playwright test --config=playwright.config.ts --grep @i18n",
    "test:seo:all": "pnpm test:unit:seo && pnpm test:e2e:seo && pnpm test:a11y && pnpm test:i18n",
    "test:e2e": "playwright test",
    "test:ci": "playwright test --workers=2 --retries=1",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "compare:products": "ts-node scripts/compare-products.ts",
    "seo-check": "tsx scripts/seo-check.ts",
    "check:sitemap": "tsx scripts/check-sitemap-robots.ts",
    "check:links": "tsx scripts/check-links-meta.ts",
    "seo:report": "lhci autorun --config=.lighthouserc.cjs",
    "seo:report:dev:desktop": "cross-env LHCI_BASE=http://localhost:3000 LHCI_FORM_FACTOR=desktop lhci autorun --config=.lighthouserc.cjs",
    "seo:report:dev:mobile": "cross-env LHCI_BASE=http://localhost:3000 LHCI_FORM_FACTOR=mobile lhci autorun --config=.lighthouserc.cjs",
    "seo:report:prod:desktop": "cross-env LHCI_BASE=https://poj-pro.uz LHCI_FORM_FACTOR=desktop lhci autorun --config=.lighthouserc.prod.cjs",
    "seo:report:prod:mobile": "cross-env LHCI_BASE=https://poj-pro.uz LHCI_FORM_FACTOR=mobile lhci autorun --config=.lighthouserc.prod.cjs",
    "seo:gsc:urls": "tsx scripts/gsc-generate-urls.ts",
    "seo:sitemap:ping": "tsx scripts/ping-sitemaps.ts",
    "content:links": "tsx scripts/generate-internal-links.ts --dir content/articles --locales ru,uz,en",
    "ci:seo": "pnpm test:unit:seo && pnpm test:e2e:seo && pnpm run check:sitemap && pnpm run check:links && pnpm build && pnpm run seo:report:dev:desktop && pnpm run seo:report:dev:mobile && pnpm run seo:report:prod:desktop && pnpm run seo:report:prod:mobile",
    "get-specs": "tsx scripts/get-specs.ts",
    "test-node": "node -e \"console.log('Node is running')\"",
    "sanitize:public": "node scripts/sanitize-public.mjs"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@react-three/drei": "^10.7.5",
    "@react-three/fiber": "^9.3.0",
    "@sentry/nextjs": "^10.3.0",
    "@swc/helpers": "^0.5.17",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/redis": "^4.0.10",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.3.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.7.0",
    "leaflet": "^1.9.4",
    "lightningcss": "1.30.1",
    "lucide-react": "^0.542.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "next-i18next": "^15.4.2",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.6.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "reading-time": "^1.5.0",
    "redis": "^5.8.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "sharp": "^0.34.3",
    "swr": "^2.3.6",
    "three": "^0.180.0",
    "unified": "^11.0.5",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0-beta.1",
    "@testing-library/user-event": "^14.6.0",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "10.4.x",
    "axe-playwright": "^2.0.1",
    "critters": "^0.0.23",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "fs-extra": "^11.3.1",
    "gray-matter": "^4.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^10.4.0",
    "postcss": "8.4.x",
    "prisma": "^6.13.0",
    "rimraf": "^5.0.7",
    "tailwindcss": "3.4.x",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.4",
    "typescript": "~5.6.0",
    "uuid": "^11.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
